{
  "name": "fs-jetpack",
  "description": "Better file system API",
  "version": "0.11.0",
  "author": "Jakub Szwacz <jakub@szwacz.com>",
  "dependencies": {
    "denodeify": "^1.2.1",
    "minimatch": "^3.0.3",
    "mkdirp": "^0.5.1",
    "q": "^1.4.1",
    "rimraf": "^2.5.4"
  },
  "devDependencies": {
    "@types/node": "^7.0.5",
    "chai": "^3.5.0",
    "codecov": "^1.0.1",
    "eslint": "^2.13.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.9.2",
